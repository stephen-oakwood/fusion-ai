

type Query {
  placeholder: String
}

type Subscription {
    agentTaskExecute(task: TaskInput): AgentResponse!
}

type AgentResponse {
    parts: [Part!]!
    responseType: String
    responseState: String
}

union Part = TextPart | FilePart

type TextPart {
    text: String!
}

type FilePart {
    name: String!
    mimeType: String!
    bytes: String
    uri: String
}

input TaskInput {
    id: String!
    sessionId: String!
    message: String!
}
